	<queryString>
		<![CDATA[	SELECT
		em.nombreempresa,
		em.dirrecionempresa,
		em.correoempresa,
		pr.cantidad,
		pr.codigoproducto,
		pr.preciocompra,
		pr.nombreproducto,
		pr.precioventa,
		pr.fechavencimiento,
		( pr.preciocompra * pr.cantidad ) total 
	FROM
		producto pr
		INNER JOIN empresa em ON pr.idempresa = em.idempresa 
	GROUP BY
		em.nombreempresa,
		em.dirrecionempresa,
		em.correoempresa,
		pr.cantidad,
		pr.codigoproducto,
		pr.preciocompra,
		pr.precioventa,
		total,
		pr.fechavencimiento,
		pr.nombreproducto]]>
	</queryString>
