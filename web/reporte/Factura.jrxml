	<queryString>
		<![CDATA[SELECT em.nombreempresa,$P{total} total2, e.nombre || e.apellido empleado,v.fechaventa, cl.nombre || cl.apellido cliente, v.nfactura factura, pr.codigoproducto codigo, pr.nombreproducto producto, r.cantidadproducto cantidad, pr.precioventa precio, SUM ( r.cantidadproducto * pr.precioventa ) total FROM venta v INNER JOIN cliente cl ON v.idcliente = cl.idcliente INNER JOIN empleado e ON v.idempleado = e.idempleado INNER JOIN registros r ON r.idventa = v.idventa INNER JOIN producto pr ON r.idproducto = pr.idproducto INNER JOIN empresa em ON e.idempresa = em.idempresa WHERE v.estado = 0 and r.idventa = (SELECT idventa FROM venta WHERE nfactura = $P{codigoFactura}) GROUP BY pr.codigoproducto,v.fechaventa, pr.nombreproducto, r.cantidadproducto, pr.precioventa, v.nfactura, e.nombre || e.apellido, cl.nombre || cl.apellido, em.nombreempresa]]>
	</queryString>
